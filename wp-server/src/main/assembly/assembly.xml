            </includes>
        </fileSet>
        <fileSet>
            <directory>../wp-server/target/conf</directory>
            <outputDirectory>conf</outputDirectory>
            <includes>
                <include>*.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <directory>../wp-server/src/main/scripts</directory>
            <outputDirectory>bin</outputDirectory>
            <includes>
                <include>*.*</include>
            </includes>
        </fileSet>
        <fileSet>
            <directory>../wp-server/src/main/logs</directory>
            <outputDirectory>logs</outputDirectory>
            <includes>
                <include>start.logger</include>
              </includes>
        </fileSet>
    </fileSets>
