        <format>tar.gz</format>
    </formats>

    <fileSets>
        <fileSet>
            <directory>../wp-server/target</directory>
            <outputDirectory>/lib</outputDirectory>
            <includes>
                <include>*.jar</include>
            </includes>
        </fileSet>
        <fileSet>
            <directory>../wp-server/target/lib</directory>
            <outputDirectory>lib</outputDirectory>
